@model IEnumerable<HospitalProject2.Models.FAQsDto>
@{
    ViewBag.Title = "List";
}

<h2>FAQ List</h2>
<a class="add_btn" href="/FAQ/New">Add New FAQ</a>

<table id="FAQTb">
    <tr>
        <th>FAQ Id:</th>
        <th>Department:</th>
        <th> Question:</th>
        <th>Answer:</th>
    </tr>
    @foreach (var FAQs in Model)
    {
        <tr>
            <th>
                Name
            </th>
            <th>
                Department
            </th>
            <th>
                Question
            </th>
            <th>
                Answer
            </th>
        </tr>
        <tr>
        <td><a class="details_btn" href="/FAQs/Details/@FAQs.FAQ_id">Details </a></td>
        </tr>

    }

</table>

